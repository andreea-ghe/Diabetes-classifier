<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="gui.DecisionTreePage"
            prefHeight="327.0" prefWidth="313.0">
    <children>
        <Button onAction="#chooseFile" fx:id="chooseFile" layoutX="18.0" layoutY="43.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="274.0" text="choose data file" />
        <Button onAction="#train" fx:id="trainButton" layoutX="20.0" layoutY="205.0" mnemonicParsing="false" prefHeight="52.0" prefWidth="274.0" text="TRAIN AND VALIDATE" />
        <Button onAction="#test" fx:id="testButton" layoutX="18.0" layoutY="273.0" mnemonicParsing="false" prefHeight="52.0" prefWidth="274.0" text="TEST" />
        <HBox alignment="CENTER_LEFT" layoutX="18.0" layoutY="114.0" prefHeight="25.0" prefWidth="202.0" spacing="10.0" />
        <Label fx:id="filenameLabel" alignment="CENTER" layoutX="18.0" layoutY="75.0" prefHeight="17.0" prefWidth="274.0" text="filename" textAlignment="CENTER" wrapText="true" />
        <Slider fx:id="trainSlider" layoutX="68.0" layoutY="179.0" prefHeight="14.0" prefWidth="166.0" />
        <Label layoutX="19.0" layoutY="178.0" prefHeight="17.0" prefWidth="49.0" text="Train %" />
        <Spinner fx:id="trainSpinner" layoutX="238.0" layoutY="174.0" prefHeight="25.0" prefWidth="57.0" />
        <Label layoutX="20.0" layoutY="14.0" text="Decision Tree" />
    </children>
</AnchorPane>
